<plugin>

   <extension
         point="org.eclipse.ui.exportWizards">
      <wizard
            category="org.eclipse.papyrus.infra.core.exportcategory"
            class="org.eclipse.papyrus.notation.export.wizards.ExportAllDiagramsWizard"
            icon="icons/exportAll.png"
            id="org.eclipse.papyrus.notation.export.wizard.uml"
            name="Export All Diagrams to UML DI">
         <selection
               class="org.eclipse.core.resources.IFile"
               name="*.di">
         </selection>
         <selection
               class="org.eclipse.papyrus.infra.onefile.model.IPapyrusFile">
         </selection>
      </wizard>
   </extension>

</plugin>
