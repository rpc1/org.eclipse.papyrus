<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.papyrus.views.modelexplorer.modelexplorer.popup?after=tools">
         <menu
               label="Generate Tooling Model"
               id="org.eclipse.papyrus.uml.profile.elementtypesconfigurations.generator.ui.generateMenu">
            <command
                  commandId="org.eclipse.papyrus.uml.profile.elementtypesconfigurations.generator.ui.generateElementTypes"
                  id="org.eclipse.papyrus.uml.profile.elementtypesconfigurations.generator.ui.generateElementTypes"
                  label="Element Types..."
                  style="push">
               <visibleWhen
                      checkEnabled="false">
                   <with
                         variable="selection">
                      <test
                            property="org.eclipse.papyrus.views.modelexplorer.tester.eClass"
                            value="uml::Profile">
                      </test>
                      <iterate
                            operator="and"
                            ifEmpty="false">
                         <test
                               property="org.eclipse.papyrus.uml.tools.isRoot">
                         </test>
                      </iterate>
                   </with>
                </visibleWhen>
            </command>
            <separator
                  name="additions"
                  visible="false">
            </separator>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.eclipse.papyrus.uml.profile.elementtypesconfigurations.generator.ui.internal.handlers.GenerateElementTypesHandler"
            description="Launch a wizard for generation of an Element Type Configurations model for the selected UML profile"
            id="org.eclipse.papyrus.uml.profile.elementtypesconfigurations.generator.ui.generateElementTypes"
            name="Generate Element Types Model">
      </command>
   </extension>

</plugin>
